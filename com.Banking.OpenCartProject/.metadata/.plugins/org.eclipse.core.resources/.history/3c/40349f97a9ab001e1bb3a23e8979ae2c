package com.Tests;

import java.util.Arrays;

public class Student
{	
	public static void main(String[] args) {
		
		int num[] = {1,8,0,3,0,2,0,2,0};
		int count=0;
		
		for(int i=0;i<num.length;i++)
		{
			if(num[i]!=0)
			{
				num[count] =num[i];
				count++;
			}
		}
		while(count<num.length)
		{
			num[count] =0;
			count++;
		}
		
		System.out.println(Arrays.toString(num));
		
	}
}